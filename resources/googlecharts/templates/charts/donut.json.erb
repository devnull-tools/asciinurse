{
  type: 'google.visualization.PieChart',
  data: google.visualization.arrayToDataTable(<%= @csv.to_json %>),
  options: {
    title: <%= @title.to_json %>,
    legend: {
      position: 'labeled'
    },
    pieHole: 0.4
  },
  style: {
    width: <%= @width.to_json %>,
    height: <%= @height.to_json %>
  }
}
